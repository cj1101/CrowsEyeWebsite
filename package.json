    "create-env": "node scripts/create-env-file.js",
    "clean": "node scripts/clean-build.js",
    "clean-build": "node scripts/clean-build.js && npm run build",
    "clean-deploy": "npm run clean-build && firebase deploy --only hosting",
    "deploy": "npm run clean-deploy",
    "deploy:full": "npm run clean-build && firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "firebase:init": "firebase init hosting",
    "firebase:serve": "npm run clean-build && firebase serve"
